med.MoveNext
Next i
Else

i = 0
Dim emp As New ADODB.Recordset
If emp.State Then emp.Close
emp.Open "select  Distinct(e.emp_no),e.emp_name ,e.emp_classification from employee e, onboard ob where e.emp_status = 'y' and e.emp_no=ob.ob_empno and ob.location='" & main.lbllocation.Caption & "' order by e.emp_name", Cn, 3, 2
